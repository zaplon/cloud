<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="http://csioz.gov.pl/p1/erecepta/mt/v20170510" xmlns="http://csioz.gov.pl/p1/erecepta/mt/v20170510" xmlns:r="http://csioz.gov.pl/p1/erecepta/mt/v20170510" xmlns:wsp="http://csioz.gov.pl/p1/wspolne/mt/v20170510" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:import namespace="http://csioz.gov.pl/p1/wspolne/mt/v20170510" schemaLocation="wspolne.xsd"/>
	<xs:complexType name="PotwierdzenieOperacjiZapisuMT">
		<xs:annotation>
			<xs:documentation>Komunikat zawiera:
				- identyfikator zadania utworzonego w P1 związanego z obsługą zapisu pakietu recept
				ALBO
				- wynik zapisu pakietu recept.
				Na wynik zapisu pakietu recept składają się:
				- kod i klucz pakietu recept
				- klucz każdej z recept
				- wynik weryfikacji każdej z recept.</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="idZadania" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Nadany przez System P1 identyfikator zadania związanego z zapisem pakietu recept.
						Wykorzystywany przez systemy zewnętrzne w komunikacji z systemem P1 do sprawdzania statusu przetwarzania zadania oraz pobierania jego wyników.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="wynikZapisuPakietuRecept" type="WynikZapisuPakietuReceptMT" minOccurs="0" maxOccurs="1"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="RealizacjaReceptyMT">
		<xs:annotation>
			<xs:documentation>Informacje o dokumencie realizacji recepty i jego statusie.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dokumentRealizacjiRecepty" type="xs:base64Binary" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Treść dokumentu realizacji recepty (XML, HL7 CDA), zakodowana za pomocą base64.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="StatusDokumentuRealizacjiReceptyEnumMT" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Status dokumentu realizacji recepty.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="KryteriaOdczytuReceptyDoKonsultacjiMT">
		<xs:annotation>
			<xs:documentation>Kryteria odczytu recepty do konsultacji.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="numerRecepty" type="NumerReceptyOIDMT" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Numer recepty.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WynikOdczytuReceptyDoKonsultacjiMT">
		<xs:annotation>
			<xs:documentation>Wynik udostępnienia recepty usługobiorcy do konsultacji (recepta + dokument weryfikacji).</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dokumentRecepty" type="xs:base64Binary" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Zaszyfrowany dokument recepty.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="przyczynaZablokowaniaRecepty" type="PrzyczynaZablokowaniaReceptyEnumMT" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Przyczyna zablokowania recepty.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statusRecepty" type="StatusReceptyEnumMT" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Status recepty.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DaneRealizacjiDoKonsultacjiMT" type="DaneRealizacjiDoKonsultacjiMT" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DaneRealizacjiDoKonsultacjiMT">
		<xs:annotation>
			<xs:documentation>Dane pojedynczej recepty w ramach udostępnianego pakietu recept usługobiorcy do konsultacji.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dataRealizacjiRecepty" type="xs:dateTime" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Data wystawienia dokumentu realizacji</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="iloscWydanegoLeku" type="xs:decimal" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Ilośc wydanego leku w ramach realizacji</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nazwaLekuRealizacja" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Nazwa leku wydanego.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="numerRecepty" type="NumerReceptyOIDMT" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Numer recepty.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WynikiWyszukiwaniaReceptUslugobiorcyDoKonsultacjiMT">
		<xs:annotation>
			<xs:documentation>Lista wyników wyszukiwania recept usługobiorcy do konsultacji.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="WynikWyszukiwaniaReceptUslugobiorcyDoKonsultacjiMT" type="WynikWyszukiwaniaReceptUslugobiorcyDoKonsultacjiMT" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WynikWyszukiwaniaReceptUslugobiorcyDoKonsultacjiMT">
		<xs:annotation>
			<xs:documentation>Pojedynczy wynik wyszukiwania recepty usługobiorcy do konsultacji.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dataWystawieniaRecepty" type="xs:dateTime" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Data wystawienia recepty (przez datę wystawienia recepty rozumie się moment zapisu recepty w Systemie P1).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="identyfikatorPodmiotuWystawcy" type="wsp:IdentyfikatorPodmiotuOIDMT" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identyfikator podmiotu wystawcy dokumentu recepty.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="identyfikatorPracownikaWystawcy" type="wsp:IdentyfikatorPracownikaMedycznegoOIDMT" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identyfikator pracownika medycznego wystawcy dokumentu recepty.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lekNazwa" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Nazwa leku w recepcie</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="numerRecepty" type="NumerReceptyOIDMT" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Numer recepty nadany przez usługodawcę wystawiającego receptę.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="podmiotNazwa" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Nazwa podmiotu wystawcy dokumentu recepty.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="przyczynaZablokowaniaRecepty" type="PrzyczynaZablokowaniaReceptyEnumMT" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Przyczyna zablokowania recepty</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statusRecepty" type="StatusReceptyEnumMT" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Status recepty.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="wystawcaNazwa" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Imię, nazwisko pracownika medycznego wystawiajacego receptę.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="KryteriaWyszukiwaniaRealizacjiReceptDoKonsultacjiMT">
		<xs:annotation>
			<xs:documentation>Kryteria wyszukiwania realizacji recept usługobiorcy do konsultacji.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dataRealizacjaReceptyDo" type="xs:dateTime" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Górne ograniczenie daty wystawienia realizacji recepty - data do kiedy wystawione mają być poszukiwane realizacje recepty. Wartość w tym polu musi być nie mniejsza niż data podana w dolnym ograniczeniu daty wystawienia realizacji recepty (o ile podano dolne ograniczenie).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dataRealizacjiReceptyOd" type="xs:dateTime" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Dolne ograniczenie daty wystawienia realizacji recepty - data od kiedy wystawione mają być poszukiwane realizacje recepty. Wartość w tym polu musi być nie większa niż data podana w górnym ograniczeniu daty wystawienia realizacji recepty (o ile podano górne ograniczenie).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="idUslugobiorcy" type="wsp:IdentyfikatorUslugobiorcyOIDMT" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identyfikator usługobiorcy, dla którego została wystawiona realizacja recepty.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="numerRecepty" type="NumerReceptyOIDMT" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Numer recepty nadany przez usługodawcę wystawiającego receptę.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WynikWyszukiwaniaRealizacjiReceptUslugobiorcyDoKonsultacjiMT">
		<xs:annotation>
			<xs:documentation>wynik wyszukiwania dokumentów realizacji recepty usługobiorcy do konsultacji.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DaneRealizacjiDoKonsultacjiMT" type="DaneRealizacjiDoKonsultacjiMT" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WynikWeryfikacjiZbioruRegulMT">
		<xs:annotation>
			<xs:documentation>Wynik weryfikacji zbioru reguł uruchamianych w ramach weryfikacji dokumentu. Poza zbiorczym wynikiem weryfikacji jest również lista wyników z poszczególnych reguł.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="wynikWeryfikacji" type="KodWynikuWeryfikacjiEnumMT" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Zbiorczy wynik weryfikacji dokumentu. Jeśli co najmniej jedna reguła zbioru reguł zakończyła sie z wynikiem błędnym to zbiorczy wynik wreyfikacji również wskazuje błąd. Analogicznie obsługiwane są ostrzeżenia. Jeśli żadna reguła nie zwróciła błędu lub ostrzeżenia wówczas zbiorczy wynik weryfikacji dla dokumentu jest pozytywny.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="identyfikatorZbioruRegul" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identyfikator zbioru reguł, które są uruchamiane w ramach weryfikacji dokumentu. Identyfikator pozwala na wskazanie dokładnej wersji konfiguracji, która została zastosowana. Identyfikator to ciąg x.y[.z] gdzie x-główny numer wersji; y-pomocniczy numer wersji; z - fix/build/patch (opcjonalny). pierwsza wersja 0.1, w x, y i z nie stosujemy 0 wiodących (tzn. nie tworzymy wersji 01.034 tylko 1.34).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="wynikWeryfikacjiReguly" type="WynikWeryfikacjiRegulyMT" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="StatusReceptyEnumMT">
		<xs:restriction base="xs:string">
			<xs:enumeration value="WYSTAWIONA"/>
			<xs:enumeration value="ZABLOKOWANA"/>
			<xs:enumeration value="ZREALIZOWANA"/>
			<xs:enumeration value="CZESCIOWO_ZREALIZOWANA"/>
			<xs:enumeration value="ANULOWANA"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StatusDokumentuRealizacjiReceptyEnumMT">
		<xs:annotation>
			<xs:documentation>Statusy dokumentów realizacji recepty</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="OBOWIAZUJACY"/>
			<xs:enumeration value="SKORYGOWANY"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RodzajRealizacjiEnumMT">
		<xs:annotation>
			<xs:documentation>Rodzaje realizacji</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="CZESCIOWA"/>
			<xs:enumeration value="WYCOFUJACA"/>
			<xs:enumeration value="ZAMYKAJACA"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PrzyczynaZablokowaniaReceptyEnumMT">
		<xs:annotation>
			<xs:documentation>Przyczyny zablokowania recepty</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="W_REALIZACJI"/>
			<xs:enumeration value="WYDANIE_ZAGRAZA_ZYCIU_LUB_ZDROWIU">
				<xs:annotation>
					<xs:documentation>Wydanie leku może zagrażać życiu lub zdrowiu pacjenta.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PRODUKT_MOZE_ZOSTAC_UZYTY_W_CELU_POZAMEDYCZNYNM">
				<xs:annotation>
					<xs:documentation>W przypadku uzasadnionego podejrzenia, że produkt leczniczy może być zastosowany w celu pozamedycznym.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ZMIANA_SKLADU_LEKU_RECEPTUROWEGO">
				<xs:annotation>
					<xs:documentation>Zachodzi konieczność dokonania zmian składu leku recepturowego, w recepcie, do których farmaceuta albo technik farmaceutyczny nie posiada uprawnień, i nie ma możliwości porozumienia się z osobą uprawnioną do wystawiania recept.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PRZETERMINOWANIE_LEKU">
				<xs:annotation>
					<xs:documentation>Od dnia sporządzenia produktu leczniczego upłynęło co najmniej 6 dni – w przypadku leku recepturowego sporządzonego na podstawie recepty lub etykiety aptecznej.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WIEK_OSOBY_DLA_KTOREJ_JEST_WYSTAWIONA_RECEPTA">
				<xs:annotation>
					<xs:documentation>Zachodzi uzasadnione podejrzenie co do wieku osoby, dla której została wystawiona recepta.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="KodWynikuWeryfikacjiEnumMT">
		<xs:restriction base="xs:string">
			<xs:enumeration value="urn:csioz:p1:erecepta:kodWynikuWeryfikacji:pozytywny"/>
			<xs:enumeration value="urn:csioz:p1:erecepta:kodWynikuWeryfikacji:blad"/>
			<xs:enumeration value="urn:csioz:p1:erecepta:kodWynikuWeryfikacji:ostrzezenie"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="WynikiWyszukiwaniaReceptMT">
		<xs:annotation>
			<xs:documentation>Lista wyników wyszukiwania.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="wynikWyszukiwaniaRecept" type="WynikWyszukiwaniaReceptMT" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="liczbaDokumentow" type="xs:int" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WynikiWyszukiwaniaReceptUslugobiorcyMT">
		<xs:annotation>
			<xs:documentation>Lista wyników wyszukiwania recept usługobiorcy.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="wynikWyszukiwaniaReceptUslugobiorcy" type="WynikWyszukiwaniaReceptUslugobiorcyMT" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="liczbaDokumentow" type="xs:int" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WynikiWyszukiwaniaDokumentowRealizacjiMT">
		<xs:annotation>
			<xs:documentation>Lista wyników wyszukiwania dokumentów realizacji recept spełniających kryteria wyszukiwania.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="wynikWyszukiwaniaDokumentowRealizacji" type="WynikWyszukiwaniaDokumentowRealizacjiMT" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="liczbaDokumentow" type="xs:int" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WynikiWyszukiwaniaDokumentowAnulowaniaMT">
		<xs:sequence>
			<xs:element name="wynikWyszukiwaniaDokumentowAnulowania" type="WynikWyszukiwaniaDokumentowAnulowaniaMT" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="liczbaDokumentow" type="xs:int" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WynikiWeryfikacjiPakietuReceptMT">
		<xs:sequence>
			<xs:element name="idZadania" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="wynikWeryfikacjiDokumentu" type="IdentyfikatorDokumentuIWynikWeryfikacjiMT" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WynikiWeryfikacjiKorektyDokumentuRealizacjiMT">
		<xs:annotation>
			<xs:documentation>Wynik weryfikacji korekty dokumentu realizacji recepty</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="wynikWeryfikacjiZbioruRegul" type="WynikWeryfikacjiZbioruRegulMT" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WynikiWeryfikacjiDokumentuReceptyMT">
		<xs:annotation>
			<xs:documentation>wynik weryfikacji dokumentu recepty farmaceutycznej</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="wynikWeryfikacjiZbioruRegul" type="WynikWeryfikacjiZbioruRegulMT" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WynikiWeryfikacjiDokumentuRealizacjiMT">
		<xs:annotation>
			<xs:documentation>Wynik przekazania dokumentu realizacji recepty.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="wynikWeryfikacjiZbioruRegul" type="WynikWeryfikacjiZbioruRegulMT" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Wynik weryfikacji zbioru reguł uruchamianych w ramach weryfikacji dokumentu recepty.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WynikiWeryfikacjiDokumentuAnulowaniaMT">
		<xs:annotation>
			<xs:documentation>Wynik przekazania dokumentu anulowania recepty.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="wynikWeryfikacjiZbioruRegul" type="WynikWeryfikacjiZbioruRegulMT" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WynikZapisuPakietuReceptMT">
	
		<xs:sequence>
			<xs:element name="kluczPakietuRecept" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Klucz pakietu recept nadany przez System P1. Klucz jest nadawany, tylko w przypadku w pozytywnej weryfikacji wszystkich recept w pakiecie (brak błędów).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="kodPakietuRecept" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
